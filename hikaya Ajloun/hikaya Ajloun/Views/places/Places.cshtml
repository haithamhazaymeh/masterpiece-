
@model hikaya_Ajloun.Models.place


@{
    Layout = "~/Views/Shared/Navbar and footer 1.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Places</title>
</head>
<body>

    <!--Breadcrumb Start-->
    <div class="pq-breadcrumb pq-breadcrumb-Cart pq-bg-dark pq-bg-img-4">
        <div class="container">
            <div class="row align-Models-center">
                <div class="col-lg-12">
                    <nav aria-label="breadcrumb">
                        <div class="pq-breadcrumb-title mt-5">
                            <h1>Your Cart</h1>
                        </div>
                        <div class="pq-breadcrumb-container">
                            <ol class="breadcrumb align-Models-center">
                                <li class="breadcrumb-Model"><a href="main-home.html"><i class="fa fa-home me-2"></i>Home</a></li>
                                <li class="breadcrumb-Model active">Our Services</li>
                            </ol>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </div>



    <!-- Gallery -->
    <div class="row mt-5 ms-5 me-5">
        <div class="col-lg-6 col-md-12 mb-4 mb-lg-0">
            <img src="~/Images/Places/@Model.placeImage1"
                 class="w-100 shadow-1-strong rounded mb-4"
                 alt="Boat on Calm Water" />

            <img src="~/Images/Places/@Model.placeImage2"
                 class="w-100 shadow-1-strong rounded mb-4"
                 alt="Wintry Mountain Landscape" />
        </div>

        <div class="col-lg-6 mb-4 mb-lg-0">
            <img src="~/Images/Places/@Model.placeImage3"
                 class="w-100 shadow-1-strong rounded mb-4"
                 alt="Mountains in the Clouds" />

            <img src="~/Images/Places/@Model.placeImage4"
                 class="w-100 shadow-1-strong rounded mb-4"
                 alt="Boat on Calm Water" />
        </div>

        <div class="col-lg-6 mb-4 mb-lg-0">
            <img src="~/Images/Places/@Model.placeImage5"
                 class="w-100 shadow-1-strong rounded mb-4"
                 alt="Waves at Sea" />
        </div>
    </div>
    <!-- Gallery -->




</body>
</html>
