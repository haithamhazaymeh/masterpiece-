<section class="food_section layout_padding-bottom">
    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <div class="pq-grid-container">
                    <div class="pq-filters">
                        <div class="filters pq-filter-button-group">
                            <ul>
                                @foreach (var item in Model.Item3)
                                {
                                    if (item.categoryName == "All" && item.type == "Products")
                                    {
                                        
                                            <li class="active pq-filter-btn" data-filter="*">@item.categoryName</li>

                                        


                                    }


                                }


                                @{

                                    int x2 = 0;
                                }
                                @foreach (var item in Model.Item3)
                                {
                                    x2++;
                                    if (item.categoryName != "All" && item.type == "Products")

                                    {
                                        <li class=" pq-filter-btn" data-filter=".@item.categoryName">@item.categoryName</li>

                                    }
                                    if (x2 == 6)
                                    {
                                        break;
                                    }

                                }
                            </ul>
                        </div>                                                        
                    </div>
                    <div class="pq-grid " data-next_items="3" data-initial_items="6">

                        @foreach (var item in Model.Item2)
                        {
                            <div class="pq-grid-item pq-filter-items pq-col-4  pq-crop-img @item.Category.categoryName">
                                <div class="box">
                                    <div>
                                        <div width="50%" class="img-box">
                                            <a href="/Home/SingleProduct?id=@item.productId"> <img src="~/Images/Products/@item.productImage_1" alt=""> </a>
                                        </div>
                                        <div class="detail-box">
                                            <a href="/Home/SingleProduct?id=@item.productId">
                                                <h5>
                                                    @item.productName
                                                </h5>
                                            </a>
                                            <p>
                                                @if (item.productDescription.Length > 220)
                                                {
                                                    var cutIndex = item.productDescription.LastIndexOf(" ", 220);
                                                    var cutDescription = item.productDescription.Substring(0, (cutIndex > 0 ? cutIndex : 220)) + "...";
                                                    @cutDescription
                                                }
                                                else
                                                {
                                                    @item.productDescription
                                                }
                                            </p>
                                            <div class="options">
                                                <h6>
                                                    JOD @item.price
                                                </h6>
                                                @*<a class="option1 btn btn-cart" href="@Url.Action("Buy", "Carts", new { productId = Convert.ToInt32(item.productId) })">Add To Cart</a>*@

                                                <a class="option1" href="@Url.Action("Buy", "Carts", new { productId = Convert.ToInt32(item.productId) })"> <i class="fa-solid fa-cart-shopping"> </i></a>

                                                @*<a href="/Carts/Cart/Buy?id=@item.productId">
            <i class="fa-solid fa-cart-shopping"> </i>



        </a>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                    <div class="text-center">
                        <a href="/Home/Products" class="pq-button" id="showMore">
                            <div class="pq-button-block">
                                <span class="pq-button-text">Show More</span>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>

