                <div class="pq-section pq-style-1 text-center">
                    <span class="pq-section-sub-title"># our latest news</span>
                    <h5 class="pq-section-title" id="blog" data-high_text="Blog" data-title_text="Latest Blog Posts" data-rough_color="#fd4a18" data-rough_type="underline">Latest Blog Posts</h5>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="owl-carousel owl-loaded owl-drag" data-dots="true" data-nav="false" data-desk_num="3" data-lap_num="2" data-tab_num="2" data-mob_num="1" data-mob_sm="1" data-autoplay="true" data-loop="true" data-margin="30">

                    @foreach (var item in Model.Item1.Take(9))
                    {

                        <div class="item">
                            <div class="pq-blog-post">
                                <div class="pq-post-media">
                                 <a href="/Home/SingleBlog?id=@item.articleId"> <img src="~/Images/article/@item.articleImage" alt="blog-img"></a>  
                                   
                                </div>
                                <div class="pq-blog-contain">
                                    <div class="pq-post-meta">
                                        <ul>
                                            <li class="pq-post-author"><i class="fa fa-user"></i>@item.author.authorname</li>
                                            <li class="pq-post-meta">
                                                <a><i class="fa fa-calendar"></i>January 2021</a>
                                            </li>
                                            <li class="pq-post-tag">
                                                <a href="/Home/blog"><i class="fa fa-tag"></i>@item.Category.categoryName</a>
                                            </li>
                                        </ul>
                                    </div>
                                    <h5 class="pq-blog-title">

                                        <a href="/Home/SingleBlog?id=@item.articleId">
                                            @{
                                                string articleName = item.articleName;
                                                if (articleName.Length > 62)
                                                {
                                                    int lastSpaceIndex = articleName.Substring(0, 62).LastIndexOf(" ");
                                                    articleName = articleName.Substring(0, lastSpaceIndex) + "...";
                                                }
                                                @articleName
                                            }
                                        </a>
                                    </h5>
                                    <a href="/Home/SingleBlog?id=@item.articleId" class="pq-button pq-btn-flat pq-mt-30">
                                        <div class="pq-button-block">
                                            <span class="pq-button-text">Read More</span>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>

                    }

                </div>
            </div>
        </div>
    </div>
</section>
<!--Blog End-->
